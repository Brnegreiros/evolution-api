{
  "version": 2,
  "builds": [
    {
      "src": "api/index-simple.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index-simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run vercel-build-ultra-simple",
  "installCommand": "npm install"
}