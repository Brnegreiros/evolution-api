{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30,
        "includeFiles": ["src/**", "dist/**", "prisma/**", "package.json", "tsconfig.json", "tsconfig.vercel.json", "tsup.config.ts", "vercel-build.sh"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install"
}